{
  "require": {
    "slim/slim": "4.*",
    "slim/psr7": "^1.7",
    "giggsey/libphonenumber-for-php": "^8.13",
    "php-di/php-di": "*",
    "olifanton/ton": "^1.5",
    "symfony/http-client": "^7.2",
    "guzzlehttp/guzzle": "^7.9",
    "http-interop/http-factory-guzzle": "^1.2",
    "php-http/guzzle7-adapter": "^1.1"
  },
  "require-dev": {
    "phpunit/phpunit": "^11.4.0"
  },
  "autoload": {
    "psr-4": {
      "App\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Test\\": "tests/"
    }
  },
  "config": {
    "allow-plugins": {
      "php-http/discovery": true
    }
  }
}
